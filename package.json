    "ava": "^0.14.0",
    "babel-core": "^6.7.4",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "clean-webpack-plugin": "^0.1.4",
    "css-loader": "^0.21.0",
    "esdoc": "^0.4.6",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^1.8.0",
    "eslint-friendly-formatter": "^1.2.2",
