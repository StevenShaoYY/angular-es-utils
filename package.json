{
  "name": "angular-es-utils",
  "version": "1.1.1",
  "description": "es utils design for angular1.x",
  "main": "./lib/index.js",
  "scripts": {
    "build": "rm -fr lib && NODE_ENV=production babel src --out-dir lib --ignore **/__tests__ && git add lib",
